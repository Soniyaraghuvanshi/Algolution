#include<iostream>
#include<stdlib.h>

using namespace std;

int main(){
	int n;
	cout<<"enter size of array :"<<endl;
	cin>>n;
	int arr[n],count=0;
	cout<<"enter the elements of array :"<<endl;
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	for(int i=0;i<n;i++){
		if(arr[i]<0){
			count++;
		}
	}
	cout<<"count = "<<count<<endl;
	return 0;
	
}
